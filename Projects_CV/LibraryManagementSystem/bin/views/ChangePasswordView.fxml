<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="173.0" prefWidth="512.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ChangePasswordController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="192.0" prefWidth="512.0">
         <children>
            <Label fx:id="deleteTitle" alignment="CENTER" prefHeight="17.0" prefWidth="400.0" text="Change Password">
               <font>
                  <Font size="18.0" />
               </font>
               <padding>
                  <Insets bottom="10.0" top="10.0" />
               </padding>
            </Label>
            <HBox alignment="CENTER" prefHeight="16.0" prefWidth="400.0">
               <children>
                  <Label fx:id="idLabel" alignment="CENTER" prefHeight="37.0" prefWidth="127.0" text="Enter Password">
                     <padding>
                        <Insets bottom="10.0" top="10.0" />
                     </padding>
                  </Label>
                  <PasswordField fx:id="password1" prefHeight="36.0" prefWidth="252.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="16.0" prefWidth="400.0">
               <children>
                  <Label fx:id="idLabel1" alignment="CENTER" prefHeight="37.0" prefWidth="127.0" text="Re-enter Password">
                     <padding>
                        <Insets bottom="10.0" top="10.0" />
                     </padding>
                  </Label>
                  <PasswordField fx:id="password2" prefHeight="36.0" prefWidth="252.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefHeight="53.0" prefWidth="400.0">
               <children>
                  <Label fx:id="errorMessage" alignment="CENTER" prefHeight="37.0" prefWidth="308.0">
                     <padding>
                        <Insets bottom="10.0" top="10.0" />
                     </padding>
                  </Label>
                  <Button fx:id="update" alignment="CENTER" mnemonicParsing="false" onAction="#updatePassword" prefHeight="39.0" prefWidth="85.0" text="Update">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <VBox.margin>
                        <Insets top="15.0" />
                     </VBox.margin>
                     <padding>
                        <Insets bottom="10.0" top="10.0" />
                     </padding>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#onBack" prefHeight="37.0" prefWidth="72.0" text="Back">
                     <padding>
                        <Insets bottom="10.0" top="10.0" />
                     </padding>
                     <HBox.margin>
                        <Insets left="5.0" right="20.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets top="10.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
