<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="330.0" prefWidth="462.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.DetailsController">
   <children>
      <VBox prefHeight="330.0" prefWidth="462.0">
         <children>
            <Label fx:id="title" alignment="CENTER" prefHeight="17.0" prefWidth="462.0" text="Label">
               <padding>
                  <Insets bottom="10.0" top="10.0" />
               </padding>
            </Label>
            <HBox alignment="CENTER_RIGHT" prefHeight="31.0" prefWidth="462.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#onBack" prefHeight="27.0" prefWidth="95.0" text="Back">
                     <HBox.margin>
                        <Insets bottom="5.0" right="20.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <TableView fx:id="detailsTable" prefHeight="261.0" prefWidth="462.0">
              <columns>
                <TableColumn fx:id="detailsLabel" prefWidth="182.0" />
                <TableColumn fx:id="detailsValue" minWidth="0.0" prefWidth="257.0" />
              </columns>
            </TableView>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
